{
  "name": "travel-list-vite",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx watch server/index.ts",
    "migrate": "node server/migrations/run-migrations.cjs up",
    "migrate:down": "node server/migrations/run-migrations.cjs down",
    "migrate:status": "node server/migrations/run-migrations.cjs status",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:e2e": "playwright test",
    "test:wait": "vitest",
    "test:ci": "npx vitest --run && npx playwright test"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@mantine/core": "^8.3.2",
    "@mantine/form": "^8.3.2",
    "@mantine/hooks": "^8.3.2",
    "@mantine/modals": "^8.3.2",
    "@mantine/notifications": "^8.3.2",
    "@tabler/icons-react": "^3.35.0",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@dnd-kit/core": "6.1.5",
    "@dnd-kit/sortable": "6.1.5",
    "@dnd-kit/utilities": "6.1.5",
    "@playwright/test": "^1.37.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9",
    "@types/react-router-dom": "^5.3.3",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.1.0",
    "jsdom": "^27.0.0",
    "supertest": "^7.1.4",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vite": "^5.4.10",
    "vitest": "^2.1.8",
    "umzug": "^3.8.2",
    "knex": "^2.5.1",
    "pg": "^8.10.0"
  }
}
